<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
	<kcfgfile name=""/>

	<group name="General">
		<entry name="showBackground" type="bool">
			<default>true</default>
		</entry>
		<entry name="updateInterval" type="int">
			<default>30</default>
		</entry>
		<entry name="source" type="string">
			<!-- <default>envcan|weather|Toronto, ON</default> -->
			<default></default>
		</entry>
		<entry name="fontFamily" type="string">
			<default></default>
		</entry>
		<entry name="bold" type="bool">
			<default>false</default>
		</entry>
		<entry name="showDailyBackground" type="bool">
			<default>false</default>
		</entry>
		<entry name="showNumDays" type="int">
			<label>0 will show all days available. 1 .. 14 will limit the number of days shown.</label>
			<default>5</default>
		</entry>
		<entry name="showWarnings" type="bool">
			<default>true</default>
		</entry>
		<entry name="showDetails" type="bool">
			<default>true</default>
		</entry>
		<entry name="forecastFontSize" type="int">
			<default>12</default>
		</entry>
		<entry name="tempFontSize" type="int">
			<default>60</default>
		</entry>
		<entry name="dateFontSize" type="int">
			<default>12</default>
		</entry>
		<entry name="minMaxFontSize" type="int">
			<default>12</default>
		</entry>
		<entry name="detailsFontSize" type="int">
			<default>12</default>
		</entry>
		<entry name="textColor" type="string">
			<default></default>
		</entry>
		<entry name="outlineColor" type="string">
			<default></default>
		</entry>
		<entry name="showOutline" type="bool">
			<default>false</default>
		</entry>
	</group>

</kcfg>

